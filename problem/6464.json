 k = first[i], val = second[i];<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;while (k--) {<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;vec.push_back(val);<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;}<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;}<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;else if (type[i] == 2) {<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;sort(vec.begin(), vec.end());<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;long long l = first[i] - 1, r = second[i], res = 0;<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;while (l &lt; r) {<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;res = (res + vec[l++]) % 1000000007;<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;}<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;printf(&quot;%lld\\n&quot;, res);<br>&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;&#xA0;}<br>&#xA0;&#xA0;&#xA0;&#xA0;}<br>}<br><br><br>&#x4E3A;&#x9632;&#x6B62;&#x4F60;&#x88AB;JMC&#x7684;&#x4EE3;&#x7801;&#x641E;&#x5230;&#x5934;&#x6655;&#x76EE;&#x7729;&#xFF0C;JMC&#x7279;&#x610F;&#x7ED9;&#x51FA;&#x4E86;&#x95EE;&#x9898;&#x7684;&#x6587;&#x5B57;&#x63CF;&#x8FF0;&#x3002;&#x5DF2;&#x77E5;&#x4E00;&#x5F00;&#x59CB;&#x6709;&#x4E00;&#x4E2A;&#x7A7A;&#x5E8F;&#x5217;&#xFF0C;&#x63A5;&#x4E0B;&#x6765;&#x6709;Q&#x6B21;&#x64CD;&#x4F5C;&#xFF0C;&#x6BCF;&#x6B21;&#x64CD;&#x4F5C;&#x7ED9;&#x51FA;type&#x3001;first&#x548C;second&#x4E09;&#x4E2A;&#x503C;&#x3002;&#x5F53;type&#x4E3A;1&#x65F6;&#xFF0C;&#x610F;&#x5473;&#x7740;&#x8BE5;&#x64CD;&#x4F5C;&#x5C5E;&#x4E8E;&#x7B2C;&#x4E00;&#x79CD;&#x64CD;&#x4F5C;&#xFF1A;&#x5F80;&#x5E8F;&#x5217;&#x5C3E;&#x90E8;&#x6DFB;&#x52A0;first&#x4E2A;second&#x6570;&#x3002;&#x5F53;type&#x4E3A;2&#x65F6;&#xFF0C;&#x610F;&#x5473;&#x7740;&#x8BE5;&#x64CD;&#x4F5C;&#x5C5E;&#x4E8E;&#x7B2C;&#x4E8C;&#x79CD;&#x64CD;&#x4F5C;&#xFF1A;&#x67E5;&#x8BE2;&#x5E8F;&#x5217;&#x4E2D;&#x7B2C;first&#x5C0F;&#x81F3;&#x7B2C;second&#x5C0F;&#x7684;&#x6570;&#x503C;&#x4E4B;&#x548C;&#xFF08;&#x4E00;&#x5171;&#x6709;(second - first + 1)&#x4E2A;&#x6570;&#x88AB;&#x7D2F;&#x52A0;&#xFF09;&#xFF0C;&#x5E76;&#x5C06;&#x7ED3;&#x679C;&#x5BF9;1000000007&#x53D6;&#x6A21;&#x540E;&#x8F93;&#x51FA;&#x3002;","type":"description","format":"html"},{"title":"Input","content":"&#x5355;&#x7EC4;&#x6570;&#x636E;<br>&#x7B2C;&#x4E00;&#x884C;&#x4E00;&#x4E2A;Q&#xFF08;1 &lt;= Q &lt;= 1e5&#xFF09;&#xFF0C;&#x4EE3;&#x8868;Q&#x6B21;&#x64CD;&#x4F5C;&#x3002;<br>&#x63A5;&#x4E0B;&#x6765;&#x6709;Q&#x884C;&#xFF0C;&#x6BCF;&#x884C;&#x5305;&#x542B;&#x4E09;&#x4E2A;&#x6574;&#x6570;type&#x3001;first&#x548C;second&#xFF1B;&#x5176;&#x4E2D;1 &lt;= type &lt;= 2&#x3002;&#x5F53;type&#x7B49;&#x4E8E;1&#x65F6;&#xFF0C;0 &lt;= first,second &lt; 1e9&#x3002;&#x5F53;type&#x7B49;&#x4E8E;2&#x65F6;&#xFF0C;1 &lt;= first &lt;= second&#xFF0C;&#x4E14;first&#x548C;second&#x5747;&#x4E0D;&#x5927;&#x4E8E;&#x76EE;&#x524D;&#x5DF2;&#x6DFB;&#x52A0;&#x8FDB;&#x5E8F;&#x5217;&#x7684;&#x6570;&#x7684;&#x6570;&#x91CF;&#x3002;","type":"input_format","format":"html"},{"title":"Output","content":"&#x5BF9;&#x4E8E;&#x6BCF;&#x6B21;&#x64CD;&#x4F5C;&#x4E8C;&#xFF0C;&#x5C06;&#x7ED3;&#x679C;&#x5BF9;1000000007&#x53D6;&#x6A21;&#x540E;&#x8F93;&#x51FA;&#x3002;","type":"output_format","format":"html"},{"title":"Sample Input","content":"<pre><div style=\"font-family:Courier New,Courier,monospace;\">6\n1 5 1\n1 6 3\n2 2 5\n2 4 8\n1 2 2\n2 4 8</div></pre>","type":"example","format":"html"},{"title":"Sample Output","content":"<pre><div style=\"font-family:Courier New,Courier,monospace;\">4\n11\n9</div></pre>","type":"example","format":"html"},{"title":"Source","content":" <a href=\"/search.php?field=problem&amp;key=%A1%B0%D7%D6%BD%DA%CC%F8%B6%AF-%CE%C4%D4%B6%D6%AA%D0%D0%B1%AD%A1%B1%B9%E3%B6%AB%B9%A4%D2%B5%B4%F3%D1%A7%B5%DA%CA%AE%CB%C4%BD%EC